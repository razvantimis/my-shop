datasource postgresql {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model User {
  id       Int     @id @default(autoincrement())
  name     String?
  email    String? @unique
  password String?
}

model Product {
  id          Int     @id @default(autoincrement())
  name        String?
  img         String?
  description String?
  price       Int?
}